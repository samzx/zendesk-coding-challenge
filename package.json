{
  "name": "boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Sam Xie",
  "license": "MIT",
  "scripts": {
    "build": "webpack",
    "gather": "yarn install && cd server && yarn install",
    "start": "webpack-dev-server --open --port 5555",
    "server": "cd server && npm run start",
    "test": "jest --config=jest.config.json",
    "prettier": "yarn prettier-js && yarn prettier-json && yarn prettier-scss",
    "prettier-js": "prettier --write */*.js && prettier --write */**/*.js && prettier --write */**/**/*.js",
    "prettier-json": "prettier --write *.json && prettier --write server/*.json",
    "prettier-scss": "prettier --write */**/*.scss && prettier --write */**/**/*.scss"
  },
  "dependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "^6.26.3",
    "babel-loader": "7.1.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "1.5.2",
    "babel-preset-react": "6.24.1",
    "css-loader": "0.28.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "jest": "^23.1.0",
    "jest-fetch-mock": "^1.6.3",
    "node-sass": "^4.9.0",
    "normalize.css": "^8.0.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-modal": "^3.4.5",
    "sass-loader": "6.0.6",
    "style-loader": "0.18.2",
    "webpack": "3.1.0",
    "webpack-dev-server": "2.5.1"
  },
  "devDependencies": {
    "babel-jest": "^23.0.1",
    "jest": "^23.1.0",
    "prettier": "1.13.5",
    "regenerator-runtime": "^0.11.1"
  }
}
